    display: inline;
}
#portal-globalnav li a {
    /* The normal, unselected tabs. They are all links */
                        float:left;
                        margin:0;
                        padding:0 15px 29px ; 
						*padding:0 15px 30px;
						font:bold 12px 'Lucida Grande','Lucida Sans Unicode',Geneva,Verdana,Sans-Serif;
						text-decoration:none;
						color:#BAA7A7;
						text-shadow: 1px 2px 0 rgba(0, 0, 0, 0.9);
                        }
* html #portal-globalnav li a {
                               
						     }
#portal-globalnav li.selected a {
    /* The selected tab. There's only one of this */
    background:url("current.png") no-repeat scroll center bottom transparent;
    color:#FFFFFF;
}
#portal-globalnav li a:hover {
                              border:medium none;
							  color:#FFFFFF;
                             }
#portal-globalnav li a:visited {
                                text-decoration:none;
							    }
#portal-personaltools {
    /* Bar with personalized menu (user preferences, favorites etc) */
    background-color: #000000; 
    border-top-color: #000000;
    border-top-style: &dtml-borderStyle;;
    border-top-width: &dtml-borderWidth;;
    border-bottom-color: #000000;
    border-bottom-style: &dtml-borderStyle;;
    border-bottom-width: &dtml-borderWidth;;
    line-height: 1.6em;
    color: &dtml-fontColor;;
    padding: 0em 3em 0em 3em;
    margin: 0;
    text-align: right;
    text-transform: &dtml-textTransform;;
